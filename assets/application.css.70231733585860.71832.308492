  outline: none; }

.lb-prev,
.lb-next {
  display: block;
  height: 100%;
  cursor: pointer; }

.lb-nav a.lb-prev {
  left: 0;
  float: left;
  width: 34%;
  background: url("/assets/prev.png") left 48% no-repeat;
  opacity: 0;
  -webkit-transition: opacity .6s;
  transition: opacity .6s; }

.lb-nav a.lb-prev:hover {
